{{#each quest}}
  <div class="row">
    <div class="col s12 m6">
      <div class="card green">
        <div class="card-content white-text">
          <span class="card-title">Вопрос: {{this.title}}</span>
          <p>Ответы: {{this.answer}}</p>
        </div>
      </div>
    </div>
  </div>
{{/each}}